	<defaultUnixFilePermissions>644</defaultUnixFilePermissions>
	<description>TuxGuitar is an Open Source multitrack tablature editor and player</description>
	<outputDirectory>${build_project_directory}/../../target</outputDirectory>
	<projectSchemaVersion>1.2</projectSchemaVersion>
	<saveRelativePaths>0</saveRelativePaths>
