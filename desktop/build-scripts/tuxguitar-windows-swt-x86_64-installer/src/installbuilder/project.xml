<project>
	<shortName>tuxguitar</shortName>
	<fullName>TuxGuitar</fullName>
	<version>9.99-SNAPSHOT</version>
	<installerFilename>${product_shortname}-${product_version}-windows-swt-x86_64-installer.${platform_exec_suffix}</installerFilename>
	<readmeFile></readmeFile>
	<licenseFile>${build_project_directory}/../../target/bundle/doc/LICENSE</licenseFile>
	<wrapLicenseFileText>1</wrapLicenseFileText>
	<leftImage>../../../common-resources/common-windows-installer-icons/TG_logo_163x314.png</leftImage>
	<logoImage>../../../common-resources/common-windows-installer-icons/TG_logo_48x48.png</logoImage>
	<splashImage>../../../common-resources/common-windows-installer-icons/TG_splash_500x300.png</splashImage>
	<windowsExecutableIcon>../../../common-resources/common-windows-installer-icons/TG_icon_install.ico</windowsExecutableIcon>
	<productDisplayIcon>${installdir}/dist/tuxguitar.ico</productDisplayIcon>
	<productUrlInfoAbout>https://github.com/helge17/tuxguitar/</productUrlInfoAbout>
	<productComments>TuxGuitar is an Open Source multitrack tablature editor and player</productComments>
	<productContact>https://github.com/helge17/tuxguitar/discussions</productContact>
	<productUrlHelpLink>https://github.com/helge17/tuxguitar/issues</productUrlHelpLink>
	<windowsUninstallerExecutableIcon>../../../common-resources/common-windows-installer-icons/TG_icon_uninstall.ico</windowsUninstallerExecutableIcon>
	<windowsSigningPkcs12File>${env(WIN_SIGN_P12_FILE)}</windowsSigningPkcs12File>
	<windowsSigningPkcs12Password>${env(WIN_SIGN_P12_PASS)}</windowsSigningPkcs12Password>
	<windowsSigningHashingAlgorithms>sha256</windowsSigningHashingAlgorithms>
	<defaultLanguage>auto</defaultLanguage>
	<allowLanguageSelection>1</allowLanguageSelection>
	<postInstallationScript></postInstallationScript>
	<postInstallationScriptArguments></postInstallationScriptArguments>
	<showPostInstallationScriptResult>0</showPostInstallationScriptResult>
	<preUninstallationScript></preUninstallationScript>
	<preUninstallationScriptArguments></preUninstallationScriptArguments>
	<allowedLanguages></allowedLanguages>
	<cdromPlatforms></cdromPlatforms>
	<defaultInstallationMode></defaultInstallationMode>
	<defaultUnixDirectoryPermissions>755</defaultUnixDirectoryPermissions>
	<defaultUnixFilePermissions>644</defaultUnixFilePermissions>
	<description>TuxGuitar is an Open Source multitrack tablature editor and player</description>
	<outputDirectory>${build_project_directory}/../../target</outputDirectory>
	<projectSchemaVersion>1.2</projectSchemaVersion>
	<saveRelativePaths>0</saveRelativePaths>
	<slideShowLoop>1</slideShowLoop>
	<slideShowTiming>5</slideShowTiming>
	<enableRollback>0</enableRollback>
	<startMenuGroupName>${product_fullname}</startMenuGroupName>
	<style>standard</style>
	<summary>TuxGuitar is an Open Source multitrack tablature editor and player</summary>
	<uninstallerDirectory>${installdir}</uninstallerDirectory>
	<uninstallerName></uninstallerName>
	<validatorProgram></validatorProgram>
	<vendor>TuxGuitar</vendor>
	<rebootRequired>1</rebootRequired>
	<componentList>
		<component>
			<name>default</name>
			<description>Default Component</description>
			<canBeEdited>1</canBeEdited>
			<detailedDescription></detailedDescription>
			<selected>1</selected>
			<show>1</show>
			<desktopShortcutList>
				<shortcut>
					<comment>${product_fullname}</comment>
					<exec></exec>
					<icon></icon>
					<name>${product_fullname}</name>
					<path></path>
					<platforms>windows-x64</platforms>
					<windowsExec>${installdir}/tuxguitar.exe</windowsExec>
					<windowsExecArgs></windowsExecArgs>
					<windowsIcon>${installdir}/dist/tuxguitar.ico</windowsIcon>
					<windowsPath>${installdir}/</windowsPath>
				</shortcut>
			</desktopShortcutList>
			<folderList>
				<folder>
					<description>Program Files</description>
					<destination>${installdir}</destination>
					<name>programfileswindows64</name>
					<platforms>windows-x64</platforms>
					<distributionFileList>
						<distributionFile>
							<allowWildcards>1</allowWildcards>
							<origin>${build_project_directory}/../../target/bundle/*</origin>
						</distributionFile>
					</distributionFileList>
				</folder>
			</folderList>
			<postInstallationActionList />
			<startMenuShortcutList>
				<startMenuShortcut>
					<comment>Uninstall ${product_fullname}</comment>
					<name>Uninstall ${product_fullname}</name>
					<windowsExec>${installdir}/uninstall.exe</windowsExec>
					<windowsExecArgs></windowsExecArgs>
					<windowsIcon></windowsIcon>
					<windowsPath>${installdir}/</windowsPath>
				</startMenuShortcut>
				<startMenuShortcut>
					<comment>${product_fullname}</comment>
					<name>${product_fullname}</name>
					<windowsExec>${installdir}/tuxguitar.exe</windowsExec>
					<windowsExecArgs></windowsExecArgs>
					<windowsIcon>${installdir}/dist/tuxguitar.ico</windowsIcon>
					<windowsPath>${installdir}/</windowsPath>
				</startMenuShortcut>
			</startMenuShortcutList>
		</component>
	</componentList>
	<customLanguageFileList />
	<fileList />
	<finalPageActionList />
	<parameterList>
		<directoryParameter>
			<name>installdir</name>
			<description>Installer.Parameter.installdir.description</description>
			<explanation>Installer.Parameter.installdir.explanation</explanation>
			<value>${platform_install_prefix}/${product_shortname}</value>
			<default>${platform_install_prefix}/${product_shortname}</default>
			<allowEmptyValue>0</allowEmptyValue>
			<ask>yes</ask>
			<cliOptionName>prefix</cliOptionName>
			<leftImage></leftImage>
			<mustBeWritable>yes</mustBeWritable>
			<mustExist>0</mustExist>
			<width>30</width>
			<!-- If we found an existing installation in the selected directory we set installationType=upgrade -->
			<postShowPageActionList>
				<setInstallerVariable>
					<name>project.installationType</name>
					<value>upgrade</value>
					<ruleList>
						<fileTest condition="exists" path="${installdir}"/>
					</ruleList>
				</setInstallerVariable>
			</postShowPageActionList>
		</directoryParameter>
	</parameterList>
	<platformOptionsList />
	<postInstallationActionList>
		<registrySet>
			<key>HKEY_CLASSES_ROOT\TuxGuitar.File\shell\open\command</key>
			<name></name>
			<type>REG_SZ</type>
			<value>${installdir}\tuxguitar.exe "%1"</value>
		</registrySet>
		<registrySet>
			<key>HKEY_CLASSES_ROOT\.tg</key>
			<name></name>
			<type>REG_SZ</type>
			<value>TuxGuitar.File</value>
		</registrySet>
		<registrySet>
			<key>HKEY_CLASSES_ROOT\.gtp</key>
			<name></name>
			<type>REG_SZ</type>
			<value>TuxGuitar.File</value>
		</registrySet>
		<registrySet>
			<key>HKEY_CLASSES_ROOT\.gp3</key>
			<name></name>
			<type>REG_SZ</type>
			<value>TuxGuitar.File</value>
		</registrySet>
		<registrySet>
			<key>HKEY_CLASSES_ROOT\.gp4</key>
			<name></name>
			<type>REG_SZ</type>
			<value>TuxGuitar.File</value>
		</registrySet>
		<registrySet>
			<key>HKEY_CLASSES_ROOT\.gp5</key>
			<name></name>
			<type>REG_SZ</type>
			<value>TuxGuitar.File</value>
		</registrySet>
		<registrySet>
			<key>HKEY_CLASSES_ROOT\.gpx</key>
			<name></name>
			<type>REG_SZ</type>
			<value>TuxGuitar.File</value>
		</registrySet>
		<registrySet>
			<key>HKEY_CLASSES_ROOT\.gp</key>
			<name></name>
			<type>REG_SZ</type>
			<value>TuxGuitar.File</value>
		</registrySet>
		<registrySet>
			<key>HKEY_CLASSES_ROOT\.ptb</key>
			<name></name>
			<type>REG_SZ</type>
			<value>TuxGuitar.File</value>
		</registrySet>
		<registrySet>
			<key>HKEY_CLASSES_ROOT\.tef</key>
			<name></name>
			<type>REG_SZ</type>
			<value>TuxGuitar.File</value>
		</registrySet>
	</postInstallationActionList>
	<postUninstallationActionList>
		<registryDelete>
			<key>HKEY_CLASSES_ROOT\.tg</key>
			<name></name>
		</registryDelete>
		<registryDelete>
			<key>HKEY_CLASSES_ROOT\.gtp</key>
			<name></name>
		</registryDelete>
		<registryDelete>
			<key>HKEY_CLASSES_ROOT\.gp3</key>
			<name></name>
		</registryDelete>
		<registryDelete>
			<key>HKEY_CLASSES_ROOT\.gp4</key>
			<name></name>
		</registryDelete>
		<registryDelete>
			<key>HKEY_CLASSES_ROOT\.gp5</key>
			<name></name>
		</registryDelete>
		<registryDelete>
			<key>HKEY_CLASSES_ROOT\.gpx</key>
			<name></name>
		</registryDelete>
		<registryDelete>
			<key>HKEY_CLASSES_ROOT\.gp</key>
			<name></name>
		</registryDelete>
		<registryDelete>
			<key>HKEY_CLASSES_ROOT\.ptb</key>
			<name></name>
		</registryDelete>
		<registryDelete>
			<key>HKEY_CLASSES_ROOT\.tef</key>
			<name></name>
		</registryDelete>
		<registryDelete>
			<key>HKEY_CLASSES_ROOT\TuxGuitar.File</key>
			<name></name>
		</registryDelete>
	</postUninstallationActionList>
	<preBuildActionList />
	<preInstallationActionList />
	<preUninstallationActionList />
	<slideShowImageList />
</project>
